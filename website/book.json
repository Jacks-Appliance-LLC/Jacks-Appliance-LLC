{
    "root": "./",
    "title": "Jack's Appliance, LLC",
    "description": "Appliance and AC Sales ad Repair",
    "author": "Joshua V. Lewis [Developer Corner]",
    "plugins": [
        "honkit-plugin-theme-darkening",
        "@dogatana/back-to-top-button",
        "@dogatana/honkit-plugin-page-toc-button",
        "sitemap",
        "page-footer",
        "canonical-link"
    ],
    "pluginsConfig": {
        "back-to-top-button": {
            "icon": "<i class='fa fa-angle-up'></i>"
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2,
            "icon": "<i class='fas fa-ellipsis-h'></i>"
        },
        "sitemap": {
            "url": "http://jacks-appliance-llc.developer-corner.xyz"
        },
        "canonical-link": {
            "baseURL": "https://jacks-appliance-llc.developer-corner.xyz" 
        },
        "page-footer": {
            "footer":"Copyright Â© Jack's Appliance, LLC. All Rights Reserved",
            "label": "Last modified: ",
            "format": "YYYY-MM-DD HH:mm:ss",
            "utc" : true
          }
    }
}
